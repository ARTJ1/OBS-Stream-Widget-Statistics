{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "mask-image",
          "mask-size",
          "mask-repeat",
          "mask-position"
        ]
      }
    ],
    "no-inline-styles": "off"
  }
}